{
"manifest_version": 2,
    "name": "skype2callto",
    "description": "change all skype: links to callto: links",
    "version": "1.0",
    "icons": { "16": "icon16.png",
           "24": "icon24.png",
          "128": "icon128.png" },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
            	"jquery.js",
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}